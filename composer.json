{
    "name": "starme/laravel-robot-chat",
    "description": "Sent message to bot-chat in laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Star.Me",
            "email": "zj_ng@126.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "laravel/framework": "^7.0",
        "phpunit/phpunit": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Starme\\Robot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Starme\\Robot\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Starme\\Robot\\RobotServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
